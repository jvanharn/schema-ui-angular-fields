    "debug": "^3.1.0"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@types/json-pointer": "^1.0.30",
    "@types/lodash": "^4.14.71",
    "ng-packagr": "^2.0.0",
    "rxjs": "^5.5.6",
    "typescript": "2.6.x",
    "zone.js": "^0.8.20"
  },
  "peerDependencies": {
    "@angular/core": "^5.2.0"
  }
